<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="WeatherView">
        <attr name="weather_temp" format="integer"/>
        <attr name="weather_country" format="string"/>
        <attr name="weather_city" format="string"/>
        <attr name="weather_wind" format="boolean"/>
        <attr name="weather_type" format="enum">
            <enum name="tornado" value="0"/>
            <enum name="night_rain" value="1"/>
            <enum name="night_wind" value="2"/>
            <enum name="day_rain" value="3"/>
        </attr>

    </declare-styleable>
</resources>